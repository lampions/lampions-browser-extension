{
  "manifest_version": 2,
  "name": "Mailgun",
  "description": "A small extension for defining email forwards via Mailgun.com",
  "version": "0.1",
  "permissions": [
    "storage",
    "alarms",
    "https://api.mailgun.net/v3"
  ],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png",
    "256": "img/icon256.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "options_ui": {
    "page": "options.html"
  }
}
