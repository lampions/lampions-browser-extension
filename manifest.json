{
  "manifest_version": 2,
  "name": "Mailgun",
  "description": "A small extension for defining email forwards via Mailgun.com",
  "version": "0.2.6",
  "permissions": [
    "storage",
    "alarms",
    "https://api.mailgun.net/v3"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{1f3d8c18-dc3c-442f-94ca-671b77c12be7}"
    }
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png",
    "256": "img/icon256.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html"
  }
}
